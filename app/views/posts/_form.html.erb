<%= render "layouts/errors", :object => @post %>

<%= form_for [current_admin, @post] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: "form-control" %>
  <%= f.label :text %>
  <%= f.text_area :content, class: "form-control" %>
  <%= f.submit 'Create Post', :class => "btn btn-primary" %>
<% end %>
